    "url": "https://github.com/bjarneo/instantly.git"
  },
  "homepage": "https://github.com/bjarneo/instantly",
  "bugs": {
    "url": "https://github.com/bjarneo/instantly/issues"
  },
  "devDependencies": {
    "eventsource": "^0.1.6",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "nanon": "^1.1.0",
    "sse-channel": "^2.0.0"
  },
  "dependencies": {
    "lodash.isfunction": "^3.0.6",
